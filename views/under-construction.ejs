<!-- Yapım Aşamasında Sayfası -->

<section class="py-5">
  <div class="container py-5 text-center">
    <i class="fas fa-hard-hat display-1 text-warning mb-4"></i>
    <h1 class="display-4 mb-3">Yapım Aşamasında</h1>
    <p class="lead mb-5">Bu sayfa henüz yapım aşamasındadır ve yakında kullanıma açılacaktır.</p>
    
    <div class="row justify-content-center">
      <div class="col-md-8">
        <div class="progress mb-4" style="height: 20px;">
          <div class="progress-bar progress-bar-striped progress-bar-animated bg-warning" 
               role="progressbar" 
               style="width: 75%" 
               aria-valuenow="75" 
               aria-valuemin="0" 
               aria-valuemax="100">
            75%
          </div>
        </div>
      </div>
    </div>
    
    <a href="<%= returnUrl %>" class="btn btn-primary mt-3">
      <i class="fas fa-arrow-left me-2"></i> Geri Dön
    </a>
  </div>
</section>